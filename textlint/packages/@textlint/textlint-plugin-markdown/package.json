{
  "name": "@textlint/textlint-plugin-markdown",
  "version": "5.2.6",
  "description": "Markdown support for textlint.",
  "keywords": [
    "markdown",
    "plugin",
    "textlint"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/textlint-plugin-markdown",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "license": "MIT",
  "author": "azu",
  "type": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "./module/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/"
  ],
  "scripts": {
    "build": "tsc -b && tsc -b tsconfig.module.json",
    "clean": "rimraf lib/ module/ tsconfig.tsbuildinfo tsconfig.module.tsbuildinfo",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.{js,ts}\"",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@textlint/markdown-to-ast": "^6.2.5"
  },
  "devDependencies": {
    "@textlint/kernel": "^3.3.6",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.3",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.2",
    "textlint-rule-no-todo": "^2.0.1",
    "ts-node-test-register": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "email": "azuciao@gmail.com"
